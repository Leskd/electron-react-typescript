{
  "name": "electron-typescript-react",
  "version": "0.1.3",
  "main": "./main.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/Leskd/electron-react-typescript.git"
  },
  "author": {
    "name": "name",
    "email": ""
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "redux"
  ],
  "bugs": {
    "url": "http://github.com/Leskd/electron-react-typescript/issues"
  },
  "homepage": "http://github.com/Leskd/electron-react-typescript",
  "license": "MIT",
  "scripts": {
    "start-renderer": "node scripts/start.js",
    "start-main": "cross-env NODE_ENV=development node --trace-warnings ./node_modules/webpack/bin/webpack --config ./config/webpack.config.electron.js --colors && electron ./main",
    "start": "concurrently --kill-others \"npm run start-renderer\" \"npm run start-main\"",
    "build-renderer": "node scripts/build.js",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings ./node_modules/webpack/bin/webpack --config ./config/webpack.config.electron.js --progress --profile --colors",
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "local": "electron ./main.js",
    "release-win": "npm run build && electron-builder --win --x64",
    "release-mac": "npm run build && electron-builder --mac --x64",
    "release-all": "npm run build && electron-builder --win --x64 && electron-builder --mac --x64",
    "postinstall": "electron-builder install-app-deps",
    "test": "node scripts/test.js --env=jsdom --watchAll"
  },
  "build": {
    "productName": "Electron-TypeScript-React",
    "artifactName": "${productName}_setup.${ext}",
    "appId": "com.example.app",
    "files": [
      "build/",
      "main.js",
      "package.json"
    ],
    "mac": {
      "identity": null
    }
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "connected-react-router": "^4.4.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "22.4.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "resolve": "1.6.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.3",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.3",
    "electron-devtools-installer": "^2.2.4",
    "redux-logger": "^3.0.6",
    "typescript": "^3.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/app/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/app/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
